<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />   
    <title>UNCO</title>
        <%#= stylesheet_link_tag 'eltema' %>
    	<%= stylesheet_link_tag 'mephisto' %>
	    <%= stylesheet_link_tag 'fullscreen' %>
	    <%= javascript_include_tag :defaults %>
    
  </head>
  <body id="body-account">
  
      <!-- div.header -->
      <div id="header">
        
      </div>
      <!-- /div.header -->
        
    <div id="container">    
      
      <!-- div.content -->
      <div id="fullscreen">
        <div id="flash-errors" style="display: none;"></div>
        <div id="flash-notice" style="display:none"></div>
        
        <!-- div.left -->
        <div id="main">

          <div class="little-box">
<form action="/account/login" id="login_form" method="post"><div style="margin:0;padding:0"><input name="authenticity_token" type="hidden" value="cbc6ade66ee4a3f228cd105d710ae1edd32b6cd0" /></div>  <dl>
    <dt><label for="login">Login</label></dt>
    <dd><input class="big" id="login" name="login" type="text" /></dd>
    <dt><label for="password">Password</label></dt>
    <dd><input class="big" id="password" name="password" type="password" value="" /></dd>
    <dt></dt>
    <dd>

      <input id="remember_me" name="remember_me" type="checkbox" value="1" /> 
      <label for="remember_me">Remember Me</label>
    </dd>
  </dl>
  <p class="btns"><input name="commit" type="submit" value="Sign in" /> or <a href="#" id="reset_password_link">reset password</a></p>
</form><form action="/account/forget" id="reset_password" method="post"><div style="margin:0;padding:0"><input name="authenticity_token" type="hidden" value="cbc6ade66ee4a3f228cd105d710ae1edd32b6cd0" /></div>  <p>Enter your email, and a brand new login key will be sent to you.  Click the link in the email to log in, and then change your password.</p>
  <p><input class="big" id="email" name="email" type="text" /></p>

  <p class="btns"><input name="commit" type="submit" value="Reset" /> or <a href="#" id="reset_password_cancel">cancel</a></p>
</form></div>

        </div>
        <!-- /div.left -->

      </div>
      <!-- /div.content -->
      
    </div>

  </body>
</html>

