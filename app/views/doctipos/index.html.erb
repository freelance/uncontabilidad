<h1>Listing doctipos</h1>

<table>
  <tr>
    <th>Nombre</th>
  </tr>

<% for doctipo in @doctipos %>
  <tr>
    <td><%=h doctipo.nombre %></td>
    <td><%= link_to 'Show', doctipo %></td>
    <td><%= link_to 'Edit', edit_doctipo_path(doctipo) %></td>
    <td><%= link_to 'Destroy', doctipo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New doctipo', new_doctipo_path %>
